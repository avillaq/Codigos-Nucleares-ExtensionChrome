{
    "name": "Codigos Nucleares",
    "description": "Esta extension permite buscar un manga h segun codigo ingresado.",
    "version": "1.0",
    "manifest_version": 3,

    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "64": "icons/64.png",
            "128": "icons/128.png"
          }
        },
        "icons": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "64": "icons/64.png",
            "128": "icons/128.png"
          }
    
}